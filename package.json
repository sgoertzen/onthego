{
  "name": "on-the-go",
  "version": "1.0.0",
  "private": true,
  "homepage": "https://www.goertzensonthego.com",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "testonly": "cross-env CI=true react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "format": "tsfmt -r",
    "lint": "tslint --project tsconfig.json",
    "storybook": "start-storybook -p 9009 -s testdata",
    "build-storybook": "build-storybook -s testdata",
    "deploy-storybook": "storybook-to-ghpages",
    "deploy": "npm run build && firebase deploy --only hosting && firebase deploy --only functions",
    "deploy-functions": "firebase deploy --only functions",
    "deploy-hosting": "npm run build && firebase deploy --only hosting",
    "deploy-database": "firebase deploy --only database",
    "deploy-rules": "firebase deploy --only firestore:rules",
    "update-dependencies": "ncu -u"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@date-io/date-fns": "^1.3.9",
    "@material-ui/core": "^4.3.3",
    "@material-ui/pickers": "^3.2.3",
    "@types/google-map-react": "^0.23.12",
    "@types/googlemaps": "^3.37.4",
    "@types/jest": "24.0.18",
    "@types/node": "12.7.2",
    "@types/react": "16.9.2",
    "@types/react-dom": "16.9.0",
    "@types/react-flag-icon-css": "^1.0.2",
    "@types/react-image-gallery": "^0.8.4",
    "@types/react-material-ui-form-validator": "^2.0.3",
    "@types/react-router-dom": "^4.3.5",
    "@types/storybook__react": "^4.0.2",
    "date-fns": "^2.0.1",
    "firebase": "^6.4.2",
    "google-map-react": "^1.1.4",
    "react": "^16.9.0",
    "react-circular-progressbar": "^2.0.2",
    "react-dom": "^16.9.0",
    "react-flag-icon-css": "^1.0.25",
    "react-image-gallery": "^0.9.1",
    "react-material-ui-form-validator": "^2.0.9",
    "react-player": "^1.12.0",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.1.1"
  },
  "devDependencies": {
    "tslint": "^5.19.0",
    "@storybook/addon-actions": "^5.1.11",
    "@storybook/addon-links": "^5.1.11",
    "@storybook/addons": "^5.1.11",
    "@storybook/react": "^5.1.11",
    "@storybook/storybook-deployer": "^2.8.1",
    "cross-env": "^5.2.0",
    "typescript": "3.5.3",
    "typescript-formatter": "^7.2.2"
  }
}
